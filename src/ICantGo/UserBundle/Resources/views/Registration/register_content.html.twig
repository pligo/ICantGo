{% trans_default_domain 'FOSUserBundle' %}

<form action="{{ path('fos_user_registration_register') }}" {{ form_enctype(form) }} role="form" method="POST" class="fos_user_registration_register">
    {{ form_widget(form) }}
    <div class="form-group">
        <button type="submit">{{ 'registration.submit'|trans }}</button>
    </div>
</form>
